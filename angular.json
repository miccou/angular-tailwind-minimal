{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "stackblitz-angular-tailwind-minimal": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/stackblitz-angular-tailwind-minimal",
            "index": "index.html",
            "main": "main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.json",
            "styles": ["styles.css"],
            "scripts": []
          },
          "configurations": {
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "development"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "development": {
              "browserTarget": "stackblitz-angular-tailwind-minimal:build:development"
            }
          },
          "defaultConfiguration": "development"
        }
      }
    }
  }
}
